# EchoScan Environment Example

# === Server / API ===
PORT=8000

# === Webhook Settings ===
WEBHOOK_URL=https://your-webhook-endpoint.example.com
WEBHOOK_SECRET=replace_with_your_webhook_secret

# === Jira Integration ===
JIRA_URL=https://your-domain.atlassian.net
JIRA_USER=your-jira-username
JIRA_TOKEN=your-jira-api-token
JIRA_PROJECT_KEY=ECHOSCAN

# === Vault/Storage (if applicable) ===
VAULT_PATH=/vault/
VAULT_SECRET_KEY=replace_with_your_vault_secret

# === EchoVerifier Settings ===
ECHO_VERIFIER_KEY=replace_with_your_echoverifier_key

# === Miscellaneous ===
DEBUG=false

# Add any other required secrets for downstream modules below:
# SBSH_SECRET=
# SDS1_API_KEY=
# RPS1_SECRET=
